import java.util.*;
public class PrimeNumber {
  
  
  public static void main(String[] args) { 
    
    
    int s1, s2;
    Scanner num = new Scanner();
    
    s1 = num.nextInt();
    s2 = num.nextInt();
    
    int st = (s1<s2)? s1:s2;
    int end = (s1>s2)? s1:s2;
    int count=0;
    for(int i=st, int j=2; i<=end; i++){
      if(i%j==0){
       count++;
      }
    }
    
    System.out.prinln(count);
  }
  
  /* ADD YOUR CODE HERE */
  
}
